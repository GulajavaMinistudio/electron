{
  "extends": "@electron/lint-roller/configs/markdownlint.json",
  "no-angle-brackets": true,
  "no-curly-braces": true,
  "no-inline-html": {
    "allowed_elements": [
      "br",
      "details",
      "img",
      "li",
      "summary",
      "ul",
      "unknown",
      "Tabs",
      "TabItem",
    ]
  }
}
